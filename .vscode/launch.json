{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug",
      "program": "${workspaceFolder}/competeCoding/leetcode/2023/a.out",
      "args": [
        "7 4 3 2 3 5 2 1"
      ],
      "cwd": "${workspaceFolder}"
    },
    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": [
        "3\n1,4\n2,5\n3,6\n"
      ],
      "stopAtEntry": false,
      "externalConsole": false,
      "cwd": "/root/Workspace/proj/CompeteCoding",
      "program": "/root/Workspace/proj/CompeteCoding/competeCoding/leetcode/2023/a.out",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}